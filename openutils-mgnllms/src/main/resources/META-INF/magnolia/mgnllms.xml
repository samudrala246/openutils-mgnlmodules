<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd">
<module>
  <name>mgnllms</name>
  <displayName>Openutils Magnolia LMS Module</displayName>
  <class>net.sourceforge.openutils.mgnllms.module.LMSModule</class>
  <versionHandler>net.sourceforge.openutils.mgnllms.module.LMSModuleVersionHandler</versionHandler>
  <version>${project.version}</version>
  <properties>
    <property>
      <name>net.sourceforge.openutils.mgnllms.managers.LearnerManager</name>
      <value>net.sourceforge.openutils.mgnllms.managers.impl.LearnerManagerWithMagnoliaUsers</value>
    </property>
    <property>
      <name>net.sourceforge.openutils.mgnllms.managers.ScormCMIManager</name>
      <value>net.sourceforge.openutils.mgnllms.managers.impl.MagnoliaScormCmiManager</value>
    </property>
    <property>
      <name>net.sourceforge.openutils.mgnllms.managers.LearnerActivitiesManager</name>
      <value>net.sourceforge.openutils.mgnllms.managers.impl.MagnoliaLearnerActivitiesManager</value>
    </property>
  </properties>
  <components>
    <id>main</id>
    <component>
      <type>net.sourceforge.openutils.mgnllms.module.LMSConfigurationModuleManager</type>
      <implementation>net.sourceforge.openutils.mgnllms.module.LMSConfigurationModuleManager</implementation>
    </component>
    <component>
      <type>net.sourceforge.openutils.mgnllms.module.LmsTypesManager</type>
      <implementation>net.sourceforge.openutils.mgnllms.module.LmsTypesManager</implementation>
    </component>
  </components>
  <dependencies>
    <dependency>
      <name>core</name>
      <version>4.5.11/4.5.99</version>
    </dependency>
    <dependency>
      <name>mgnlutils</name>
      <version>5.0.10/*</version>
    </dependency>
    <dependency>
      <name>criteria</name>
      <version>5.0.10/*</version>
    </dependency>
    <dependency>
      <name>adminInterface</name>
      <version>*</version>
    </dependency>
    <dependency>
      <name>templating</name>
      <version>*</version>
    </dependency>
  </dependencies>
  <repositories>
    <repository>
      <name>magnolia</name>
      <workspaces>
        <workspace>lms</workspace>
      </workspaces>
      <nodeTypeFile>/mgnl-nodetypes/mgnllms/magnolia-nodetypes.xml</nodeTypeFile>
    </repository>
  </repositories>
</module>